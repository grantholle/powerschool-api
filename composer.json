{
    "name": "grantholle/powerschool-api",
    "description": "A php package to make interacting with PowerSchool less painful.",
    "keywords": ["powerschool"],
    "license": "MIT",
    "authors": [
        {
            "name": "Grant Holle",
            "homepage": "http://grantholle.com",
            "role": "Developer"
        }
    ],
    "version": "1.4.2",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/console": "^5.6|^6.0|^7.0",
        "illuminate/support": "^5.6|^6.0|^7.0",
        "pear/openid": "^0.4.0",
        "spatie/url": "^1.3"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "GrantHolle\\PowerSchool\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrantHolle\\PowerSchool\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrantHolle\\PowerSchool\\PowerSchoolServiceProvider"
            ],
            "aliases": {
                "PS": "GrantHolle\\PowerSchool\\Api\\RequestBuilder"
            }
        }
    }
}
