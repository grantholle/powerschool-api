{
    "name": "grantholle/powerschool-api",
    "description": "A boilerplate for new composer based PHP projects.",
    "keywords": ["powerschool"],
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    }
}
