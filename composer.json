{
    "name": "grantholle/powerschool-api",
    "description": "A php package to make interacting with PowerSchool less painful.",
    "keywords": ["powerschool"],
    "license": "MIT",
    "version": "1.1.1",
    "authors": [
        {
            "name": "Grant Holle",
            "homepage": "http://grantholle.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/console": "^5.6",
        "illuminate/support": "^5.6",
        "symfony/cache": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "GrantHolle\\PowerSchool\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrantHolle\\PowerSchool\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "GrantHolle\\PowerSchool\\PowerSchoolServiceProvider"
            ],
            "aliases": {
                "PS": "GrantHolle\\PowerSchool\\Api\\RequestBuilder"
            }
        }
    }
}
