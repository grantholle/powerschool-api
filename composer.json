{
    "name": "grantholle/powerschool-api",
    "description": "A php package to make interacting with PowerSchool less painful.",
    "keywords": ["powerschool"],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Grant Holle"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.5",
        "symfony/cache": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "PowerSchool\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "PowerSchool\\PowerSchoolServiceProvider"
            ],
            "aliases": {
                "RequestBuilder": "PowerSchool\\Api\\RequestBuilder"
            }
        }
    }
}
